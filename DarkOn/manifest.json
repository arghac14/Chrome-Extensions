{
    "name": "DarkON",
    "description": "Chrome extension to make the bachground dark of any site.",
    "version": "1.0",
    "permissions": [
      "tabs", "http://*/*", "https://*/*"
    ],
    "browser_action": {
        "default_title": "Set this page dark",
        "default_icon": "./images/icon.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }